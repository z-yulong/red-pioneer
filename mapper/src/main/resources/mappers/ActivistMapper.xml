<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.imau.redpioneer.dao.ActivistMapper">

    <resultMap id="BaseResultMap" type="cn.edu.imau.redpioneer.entity.Activist">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="account" jdbcType="VARCHAR" property="account" />
    <result column="password" jdbcType="VARCHAR" property="password" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="sex" jdbcType="VARCHAR" property="sex" />
    <result column="nation" jdbcType="VARCHAR" property="nation" />
    <result column="date_of_birth" jdbcType="DATE" property="dateOfBirth" />
    <result column="native_place" jdbcType="VARCHAR" property="nativePlace" />
    <result column="education" jdbcType="VARCHAR" property="education" />
    <result column="admission_time" jdbcType="DATE" property="admissionTime" />
    <result column="address" jdbcType="VARCHAR" property="address" />
    <result column="id_card" jdbcType="VARCHAR" property="idCard" />
    <result column="tel" jdbcType="VARCHAR" property="tel" />
    <result column="application_time" jdbcType="DATE" property="applicationTime" />
    <result column="is_adult" jdbcType="VARCHAR" property="isAdult" />
    <result column="photo" jdbcType="VARCHAR" property="photo" />
    <result column="leader_stu" jdbcType="INTEGER" property="leaderStu" />
    <result column="leader_techer" jdbcType="INTEGER" property="leaderTecher" />
    <result column="juri" jdbcType="INTEGER" property="juri" />
    <result column="diploma" jdbcType="VARCHAR" property="diploma" />
    <result column="upactivist_time" jdbcType="DATE" property="upactivistTime" />
  </resultMap>

  <update id="updateInfoByPrimaryKey" parameterType="cn.edu.imau.redpioneer.entity.Activist" >
    update activist
    <set>
        <if test="account != null">account = #{account},</if>
        <if test="password != null">password = #{password},</if>
        <if test="name != null">name = #{name},</if>
        <if test="sex != null">sex = #{sex},</if>
        <if test="nation != null">nation = #{nation},</if>
        <if test="dateOfBirth != null">date_of_birth = #{dateOfBirth},</if>
        <if test="nativePlace != null">native_place = #{nativePlace},</if>
        <if test="education != null">education = #{education},</if>
        <if test="admissionTime != null">admission_time = #{admissionTime},</if>
        <if test="address != null">address = #{address},</if>
        <if test="idCard != null">id_card = #{idCard},</if>
        <if test="tel != null">tel = #{tel},</if>
        <if test="applicationTime != null">application_time = #{applicationTime},</if>
        <if test="isAdult != null">is_adult = #{isAdult},</if>
        <if test="photo != null">photo = #{photo},</if>
        <if test="leaderStu != null">leader_stu = #{leaderStu},</if>
        <if test="leaderTecher != null">leader_techer = #{leaderTecher},</if>
        <if test="juri != null">juri = #{juri},</if>
        <if test="diploma != null">diploma = #{diploma}</if>
        <if test="upactivistTime != null">upactivist_time = #{upactivistTime}</if>

    </set>
        where id=#{id}

  </update>

    <insert id="batchInsert" parameterType="java.util.List">
        INSERT INTO activist
        (account,name)
        VALUES
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.account},#{item.name})
        </foreach>
    </insert>
    


</mapper>